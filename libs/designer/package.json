    "mini-css-extract-plugin": "^2.6.1",
    "process": "^0.11.10",
    "protoc-gen-grpc-web": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "google-protobuf": "^3.21.0",
    "grpc-web": "^1.3.1",
    "paperclip-loader": "workspace:^"
