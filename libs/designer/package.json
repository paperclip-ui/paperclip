  "main": "index.js",
  "scripts": {
    "build:watch": "yarn build:pc && concurrently \"yarn build:pc --watch\" \"webpack --watch\"",
    "build:pc": "../../target/debug/paperclip_cli build",
    "build:proto": "protoc --plugin=\"protoc-gen-grpc-web=../../node_modules/.bin/protoc-gen-grpc-web\" --grpc-web_out=\"import_style=commonjs+dts,mode=grpcwebtext:./src/editor\" --js_out=\"import_style=commonjs:./src/editor\" proto/designer.proto",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "../../target/debug/paperclip_cli designer --open --port=8000"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.1.0",
