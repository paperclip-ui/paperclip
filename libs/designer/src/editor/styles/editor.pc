token gray0 #CCC

style defaultFont {
    font-family: Helvetica
    font-size: 16px
  }


style mobileFont {
  font-size: 32px
}

component Page {
  variant isMobile trigger {
    "@media screen and (max-width: 700px)"
  }
  render div {
    style extends defaultFont
    style variant isMobile extends mobileFont {
      color: var(gray0)
    }
    text "Hello worldfsdfsdmfdsddmfsfksd"
  }
}