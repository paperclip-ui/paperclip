/**
 * @bounds(x: -1412, y: 115, width: 1024, height: 768)
*/

public component Something {
    variant large trigger {
        ".large"
    }
    variant alt trigger {
        ".alt"
    }
    render div(class: class) {
        style variant alt + large {
            font-size: 42px
        }
        style variant alt {
            background: var(theme.gray05)
            color: var(theme.gray0)
        }
        style variant aVariant {
            background: var(theme.gray05)
            color: var(theme.gray0)
        }
        style {
            background: var(theme.blue02)
            color: var(theme.blue00)
            font-family: var(theme.defaultFontFamily)
            font-size: 32px
        }
        text "Something"
    }
}

