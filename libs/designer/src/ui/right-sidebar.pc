    }
    render div root(class: class) {
        style variant hover {
            background: var(module.gray04)
        }
        style {
            color: var(theme.gray02)
            text-align: right
            font-family: var(theme.defaultFontFamily)
            display: flex
            align-items: center
            justify-content: space-between
