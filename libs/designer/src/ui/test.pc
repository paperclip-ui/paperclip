import "./theme.pc" as theme

public style bMixin {
    font-family: sans-serif
}

public token color var(theme.gray0)
public style aMixin {
    color: var(color)
    font-size: 32px
    font-family: var(theme.defaultFontFamily)
    background: var(theme.gray04)
}

/**
 * @frame(x: 1380, y: -234, width: 430, height: 185)
*/
public component Something {
    variant cd
    render div {
        style variant ab {
            position: absolute
        }
        style variant ab + cd {
            position: fixed
        }
        style extends aMixin, bMixin {
            position: relative
            color: rgba(236, 57, 42, 0.72)
            font-family: something
        }
        text "something!!"
    }
}

/**
 * @frame(x: 622, y: -252, width: 456, height: 205)
*/
div {
    style extends aMixin {
        position: relative
        font-size: 32px
    }
}
