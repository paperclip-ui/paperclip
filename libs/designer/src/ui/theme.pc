public style mask {
    background-color: currentColor
    -webkit-mask-size: 100%
    -webkit-mask-repeat: no-repeat
}

public style icon extends mask {
    width: 1em
    height: 1em
    display: inline-block
}

public style input {
    background: var(gray04)
    display: flex
    outline: none
    align-items: center
    text-overflow: ellipsis
    font-size: var(fontSize01)
    font-family: inherit
    white-space: nowrap
    overflow: hidden
    flex: 1
    padding: var(space01) var(space03)
    border-radius: var(rounded)
    border: none
    color: currentColor
    min-height: var(inputHeight)
}

public style card {
    border: 1px solid var(borderColor)
    border-radius: var(roundedMD)
    background: rgba(37, 37, 37, 0.8)
    backdrop-filter: blur(10px)
}

public style inputActive {
    box-shadow: inset 0px 0px 0px 1px var(blue01)
}

public style popup {
    background: var(popupBackgroundColor)
    border-radius: var(rounded)
    filter: drop-shadow(5px 5px 8px rgba(0, 0, 0, 0.7))
    border: 1px solid var(borderColor)
}

public style fontDefault {
    font-family: Inter, sans-serif
    color: var(fontColor01)
    font-size: var(fontSize01)
}

public style reset {
    border: none
    background: transparent
    color: currentColor
    padding: 0
    margin: 0
}

public style fontHeader {
    font-size: var(fontSize01)
    font-weight: 600
}

public style easeIn {
    transition: 0.2s
}

public style centered {
    left: 50%
    top: 50%
    position: absolute
    transform: translate(-50%, -50%)
}

trigger mobile {
    "@media screen and (max-width: 480px)"
}
