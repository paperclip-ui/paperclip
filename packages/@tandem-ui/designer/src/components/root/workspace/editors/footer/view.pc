/**
 * @bounds(left: 440, top: 2365, right: 806, bottom: 2397)
 */
public component Breadcrumb {
  variant component (enabled: [])
  variant shadow (enabled: [])
  variant text (enabled: [])
  variant element2 (enabled: [])
  variant plug (enabled: [])
  variant slot (enabled: [])
  variant hovering (enabled: [PseudoElement.Hover])
  variant selected (enabled: [true])
  render div {
    style {
      include imp1.baseFont
      display: flex
      align-items: center
      cursor: pointer
    }
    div labelContainer {
      style {
        background-image: linear-gradient(#{imp0.grey2}, #{imp0.grey2})
        display: flex
        height: 100%
        align-items: center
        padding-left: 36px
        padding-right: 0px
      }
      style variant hovering {
        box-shadow: unset
        padding-left: 36px
      }
      style variant selected {
        background-image: linear-gradient(#{imp0.blue1}, #{imp0.blue1})
        padding-right: 24px
      }
      div icon {
        style {
          margin-right: 6px
        }
        style variant component {
        }
        style variant hovering {
          color: #{imp0.blue1}
        }
        style variant selected {
          color: #{imp0.grey0}
        }
        imp2.ReplaceIcon replaceIcon {
          style {
            display: none
          }
          style variant slot {
            display: block
            height: 1em
          }
        }
        imp2.ShadowIcon shadowIcon {
          style {
            display: none
          }
          style variant shadow {
            display: block
            height: 1.2em
          }
        }
        imp2.ComponentIcon componentIcon {
          style {
            display: none
          }
          style variant component {
            display: block
            height: 1em
          }
        }
        imp2.EmptySquareIcon emptySquareIcon {
          style {
            display: none
            height: 1em
          }
          style variant element2 {
            display: block
          }
        }
        imp2.TextIcon textIcon {
          style {
            display: none
            height: 18px
            position: relative
            top: 3px
          }
          style variant text {
            display: block
          }
        }
        imp2.DottedBoxIcon dottedBoxIcon {
          style {
            display: none
            height: 1em
          }
          style variant plug {
            display: block
          }
        }
      }
      text label "layer name" {
        style {
          cursor: pointer
          white-space: nowrap
        }
        style variant hovering {
          color: #{imp0.blue1}
        }
        style variant selected {
          color: #{imp0.grey0}
        }
      }
    }
    div arrowContainer {
      style {
        box-sizing: border-box
        display: block
        flex-shrink: 0
        position: relative
      }
      style variant selected {
        display: block
      }
      div arrow {
        style {
          box-sizing: border-box
          display: inline-block
          border-top: 50px solid rgba(255, 0, 0, 0)
          border-right: 0px solid rgba(230, 230, 230, 0)
          border-bottom: 50px solid rgba(255, 0, 0, 0)
          border-left: 25px solid #{imp0.grey2}
          position: absolute
          transform: translateY(-50%)
          filter: drop-shadow(1px 0px 0px #{imp0.grey1})
        }
        style variant hovering {
          filter: drop-shadow(1px 0px 0px #{imp0.blue1})
        }
        style variant selected {
          display: none
        }
      }
    }
  }
}

