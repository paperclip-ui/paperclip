{
  "name": "tandem-desktop",
  "productName": "Tandem",
  "version": "10.1.35",
  "description": "",
  "private": true,
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crcn/tandem-preview.git"
  },
  "bin": {
    "tandem": "./bin/tandem"
  },
  "scripts": {
    "start": "electron lib/index.js",
    "postinstall": "electron-rebuild",
    "build": "tsc && webpack",
    "build:watch": "concurrently \"tsc --watch\" \"webpack --watch\"",
    "build-front-end": "webpack",
    "clean": "rm -rf lib && rm -rf node_modules",
    "prepare": "npm run build",
    "build:dist": "electron-packager . \"Tandem\" --out=dist --overwrite --app-bundle-id=\"com.tandemcode.tandem\" --icon=\"assets/tandem.icns\" --osx-sign",
    "build:dist:win": "build"
  },
  "build": {
    "appId": "com.tandemcode.app",
    "fileAssociations": [
      {
        "ext": "tdproject",
        "name": "Tandem Project",
        "role": "Editor"
      }
    ],
    "win": {
      "target": "zip"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/browser": "^4.4.0",
    "@types/raven": "^2.5.1",
    "chokidar": "^2.0.4",
    "concurrently": "4.1.x",
    "crc32": "^0.2.2",
    "fix-path": "^2.1.0",
    "fs-extra": "^6.0.1",
    "fsbox": "^10.1.33",
    "get-port": "^3.2.0",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.20",
    "paperclip": "^10.1.33",
    "paperclip-migrator": "^10.1.33",
    "process": "^0.11.10",
    "raven-js": "^3.27.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "tandem-common": "^10.1.33",
    "tandem-front-end": "^10.1.35",
    "terminate": "^2.1.2",
    "update-electron-app": "^1.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.2",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^0.28.11",
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3",
    "electron-packager": "^15.5.1",
    "electron-rebuild": "^3.2.7",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "5.5.0",
    "null-loader": "^0.1.1",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-loader": "^0.21.0",
    "ts-loader": "5.1.0",
    "url-loader": "^1.0.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "worker-loader": "^2.0.0",
    "wrapper-webpack-plugin": "^2.0.0"
  }
}
